escape Super_L
bind s-Return exec urxvt
bind s-d exec rofi -show run 
bind s-E exec sh ~/scripts/killX.sh
bind s-w exec sh ~/scripts/vim_directory.sh
unbind 0
unbind 1
unbind 2
unbind 3
unbind 4
unbind 5
unbind 6
unbind 7
unbind 8
unbind 9
unbind s-k
bind s-Q kill
bind s-1 select 0
bind s-2 select 1
bind s-3 select 2
bind s-4 select 3
bind s-5 select 4
bind s-6 select 5
bind s-7 select 6
bind s-8 select 7
bind s-9 select 8
bind s-0 select 9
startup_message off
